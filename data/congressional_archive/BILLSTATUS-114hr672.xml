    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2015-02-03</actionDate>
          <text><![CDATA[<p><b>Rural Community Hospital Demonstration Extension Act of 2015</b></p> <p>Amends the Medicare Prescription Drug, Improvement, and Modernization Act of 2003, as amended by the Patient Protection and Affordable Care Act, to extend the period of the rural community hospital demonstration program from 5 to 10 years.</p>]]></text>
          <lastSummaryUpdateDate>2015-03-13T17:51:34Z</lastSummaryUpdateDate>
          <actionDesc>Introduced in House</actionDesc>
          <name>Introduced in House</name>
          <updateDate>2015-02-03T05:00:00Z</updateDate>
        </item>
      </billSummaries>
    </summaries>
