    <summaries>
      <billSummaries>
        <item>
          <actionDesc>Introduced in Senate</actionDesc>
          <actionDate>2013-05-16</actionDate>
          <updateDate>2013-05-16T04:00:00Z</updateDate>
          <versionCode>00</versionCode>
