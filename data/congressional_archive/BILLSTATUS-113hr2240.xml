      <billSummaries>
        <item>
          <lastSummaryUpdateDate>2013-06-21T20:05:51Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
          <actionDesc>Introduced in House</actionDesc>
          <text><![CDATA[<p>Small Business Tax Equity Act of 2013 - Amends the Internal Revenue Code to exempt a trade or business that conducts marijuana sales in compliance with state law from the prohibition against allowing business-related tax credits or deductions for expenditures in connection with trafficking in controlled substances. </p>]]></text>
          <actionDate>2013-06-04</actionDate>
          <versionCode>00</versionCode>
          <updateDate>2013-06-04T04:00:00Z</updateDate>
        </item>
