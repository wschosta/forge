    <title>Expressing support for designation of January 2014 as "National Blood Donor Month".</title>
    <policyArea>
      <name>Health</name>
    </policyArea>
    <constitutionalAuthorityStatementText />
    <recordedVotes />
    <createDate>2014-01-30T07:01:58Z</createDate>
