      <billSummaries>
        <item>
          <actionDesc>Introduced in Senate</actionDesc>
          <actionDate>2014-04-30</actionDate>
          <updateDate>2014-04-30T04:00:00Z</updateDate>
          <versionCode>00</versionCode>
