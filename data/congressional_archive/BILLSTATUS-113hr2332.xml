      <billSummaries>
        <item>
          <updateDate>2013-06-12T04:00:00Z</updateDate>
          <text><![CDATA[<p>Adoption Tax Credit Tribal Parity Act of 2013 - Amends the Internal Revenue Code to allow Indian tribes to make the determination that a child is a child with special needs for purposes of the adoption tax credit.</p>]]></text>
          <actionDate>2013-06-12</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <name>Introduced in House</name>
          <lastSummaryUpdateDate>2013-06-28T20:16:49Z</lastSummaryUpdateDate>
          <versionCode>00</versionCode>
        </item>
      </billSummaries>
