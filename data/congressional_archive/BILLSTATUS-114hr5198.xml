      <billSummaries>
        <item>
          <actionDate>2016-05-11</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2016-09-19T21:17:52Z</lastSummaryUpdateDate>
          <versionCode>00</versionCode>
