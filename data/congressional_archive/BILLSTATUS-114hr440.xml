    <summaries>
      <billSummaries>
        <item>
          <actionDate>2015-01-21</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2015-02-04T23:15:18Z</lastSummaryUpdateDate>
          <versionCode>00</versionCode>
