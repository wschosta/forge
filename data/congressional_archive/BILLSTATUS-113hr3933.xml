    <summaries>
      <billSummaries>
        <item>
          <actionDesc>Introduced in House</actionDesc>
          <actionDate>2014-01-27</actionDate>
          <updateDate>2014-01-27T05:00:00Z</updateDate>
          <versionCode>00</versionCode>
