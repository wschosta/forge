    <summaries>
      <billSummaries>
        <item>
          <actionDesc>Introduced in Senate</actionDesc>
          <actionDate>2014-06-11</actionDate>
          <updateDate>2014-06-11T04:00:00Z</updateDate>
          <versionCode>00</versionCode>
