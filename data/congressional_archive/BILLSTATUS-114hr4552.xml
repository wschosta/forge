      <item>
        <title>To require all gas stations offering self-service to meet certain accessibility standards for individuals with disabilities, and for other purposes.</title>
        <chamberName />
        <chamberCode />
        <titleType>Official Title as Introduced</titleType>
        <parentTitleType />
      </item>
      <item>
        <title>Gas Pump Access Act of 2016</title>
        <chamberName />
