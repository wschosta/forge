    <summaries>
      <billSummaries>
        <item>
          <updateDate>2015-06-24T04:00:00Z</updateDate>
          <versionCode>00</versionCode>
          <text><![CDATA[<p>This bill amends the Consolidated Natural Resources Act of 2008 to add Livingston County, the city of Jonesboro in Union County, and the city of Freeport in Stephenson County in Illinois to the Abraham Lincoln National Heritage Area. </p>]]></text>
          <lastSummaryUpdateDate>2015-11-23T21:44:15Z</lastSummaryUpdateDate>
          <actionDate>2015-06-24</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
    </summaries>
