    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2015-02-10</actionDate>
          <text><![CDATA[<p><b>Lena Horne Recognition Act</b></p> <p>Directs the Speaker of the House of Representatives and the President pro tempore of the Senate to arrange for the posthumous presentation of a Congressional Gold Medal in commemoration of Lena Horne in recognition of her achievements and contributions to American culture and the civil rights movement.</p>]]></text>
          <lastSummaryUpdateDate>2015-02-28T15:39:30Z</lastSummaryUpdateDate>
          <actionDesc>Introduced in House</actionDesc>
          <name>Introduced in House</name>
          <updateDate>2015-02-10T05:00:00Z</updateDate>
        </item>
      </billSummaries>
    </summaries>
