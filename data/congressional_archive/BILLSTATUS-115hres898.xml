    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <actionDate>2018-05-16</actionDate>
          <actionDesc>Introduced in House</actionDesc>
          <name>Introduced in House</name>
          <lastSummaryUpdateDate>2018-05-18T16:45:39Z</lastSummaryUpdateDate>
          <text><![CDATA[<p class="lbexIndent" style="TEXT-ALIGN: left">Directs the Department of Homeland Security (DHS) to transmit to the House of&nbsp; Representatives copies of any document, record, memo, correspondence, or other communication of&nbsp; DHS or any portion of any such communication, that refers or relates to the use of ZTE Corporation's cybersecurity products.</p> <p class="lbexIndent" style="TEXT-ALIGN: left"></p>]]></text>
          <updateDate>2018-05-16T04:00:00Z</updateDate>
        </item>
        <item>
          <versionCode>79</versionCode>
          <actionDate>2018-06-08</actionDate>
          <actionDesc>Reported to House without amendment</actionDesc>
          <name>Reported to House without amendment</name>
          <lastSummaryUpdateDate>2018-06-12T21:40:52Z</lastSummaryUpdateDate>
          <text><![CDATA[<p>Directs the Department of Homeland Security (DHS) to transmit to the House of&nbsp; Representatives copies of any document, record, memo, correspondence, or other communication of&nbsp; DHS or any portion of any such communication, that refers or relates to the use of ZTE Corporation's cybersecurity products.</p> <p class="lbexIndent" style="TEXT-ALIGN: left"></p>]]></text>
          <updateDate>2018-06-08T12:40:24Z</updateDate>
        </item>
      </billSummaries>
