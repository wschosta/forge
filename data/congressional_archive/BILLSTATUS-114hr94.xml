    <summaries>
      <billSummaries>
        <item>
          <actionDate>2015-01-06</actionDate>
          <name>Introduced in House</name>
          <actionDesc>Introduced in House</actionDesc>
          <updateDate>2015-01-06T05:00:00Z</updateDate>
          <text><![CDATA[<p><b>Cameras in the Courtroom Act</b></p> <p>Requires the Supreme Court to permit television coverage of all open sessions of the Court unless it decides by majority vote that allowing such coverage in a particular case would violate the due process rights of any of the parties involved.</p>]]></text>
          <lastSummaryUpdateDate>2015-01-12T19:16:46Z</lastSummaryUpdateDate>
          <versionCode>00</versionCode>
        </item>
      </billSummaries>
